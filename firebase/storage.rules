rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Contract uploads - authenticated users only
    match /contracts/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
    
    // Processed outputs - authenticated users only
    match /processed/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}